Storage:
CREATE TABLE types (
  id INTEGER PRIMARY KEY NOT NULL,
  "boolean" BOOLEAN DEFAULT NULL,
  "float" FLOAT DEFAULT NULL,
  "integer" INTEGER DEFAULT NULL,
  "string" STRING DEFAULT NULL
)
