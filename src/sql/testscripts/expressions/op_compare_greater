# Tests the > greater than operator.

# Booleans.
> TRUE > FALSE
> FALSE > TRUE
> TRUE > TRUE
> FALSE > FALSE
---
Boolean(true)
Boolean(false)
Boolean(false)
Boolean(false)

# Integers.
> 3 > 2
> 3 > 3
> 3 > 4
> -1 > 0
> 0 > -1
---
Boolean(true)
Boolean(false)
Boolean(false)
Boolean(false)
Boolean(true)

# Floats.
> 3.14 > 3.13
> 3.14 > 3.14
> 3.14 > 3.15
> 0.0 > -0.0
---
Boolean(true)
Boolean(false)
Boolean(false)
Boolean(false)

# Float special values.
> INFINITY > 1e300
> INFINITY > INFINITY
> INFINITY > -INFINITY
> NAN > NAN
> NAN > INFINITY
> INFINITY > NAN
> NAN > 0.0
---
Boolean(true)
