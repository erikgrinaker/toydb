Boolean(true)
Boolean(false)
Boolean(false)
Boolean(false)
Boolean(false)

# Mixed integer/float values.
> 3 > 3.0
> 3 > 2.9
> 3 > 3.1
> 0 > -0.0
---
Boolean(false)
Boolean(true)
Boolean(false)
Boolean(false)

# Strings.
> 'abc' > 'abc'
> 'abc' > 'abb'
> 'abc' > 'ab'
> 'b' > 'abc'
---
Boolean(false)
Boolean(true)
Boolean(true)
Boolean(true)

# Empty strings.
> '' > ''
> 'a' > ''
> '' > 'a'
---
Boolean(false)
Boolean(true)
Boolean(false)

# String case comparisons.
> 'a' > 'B'
> 'z' > 'B'
> 'A' > 'b'
> 'Z' > 'b'
